{
  "name": "sento-transition",
  "version": "0.1.0",
  "description": "A react component for css animation",
  "author": "Frederic Godin",
  "repository": "Epimodev/sento-transition",
  "license": "MIT",
  "main": "module/index.js",
  "module": "module/index.js",
  "typings": "module/index.d.ts",
  "keywords": [
    "react",
    "css",
    "transition",
    "animation",
    "typescript"
  ],
  "files": [
    "src",
    "module"
  ],
  "scripts": {
    "build": "rm -rf ./module && tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts?(x)'",
    "check-format": "prettier --list-different 'src/**/*.ts?(x)'",
    "storybook": "start-storybook -s ./ -p 9001 -c .storybook",
    "prepare": "yarn lint && yarn check-format && yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn check-format"
    }
  },
  "peer-dependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@emotion/core": "^0.13.1",
    "@storybook/react": "^4.0.0-alpha.24",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "husky": "^1.1.2",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.1.2"
  }
}
